def diagonalDifference(arr):
    # Write your code here
    l_dia_sum=r_dia_sum=0
    for i in range(len(arr)):
        for j in range(len(arr)):
            if (i==j):
                l_dia_sum +=arr[i][j]
            if (j==n-i-1):
                r_dia_sum+=arr[i][j]
    return (abs(l_dia_sum-r_dia_sum))
