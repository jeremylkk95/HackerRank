def kangaroo(x1, v1, x2, v2):
    # Write your code here
    Exist = False
    k1_dis = k2_dis = 0
    for i in range(1,10000):
        k1_dis = x1+(i*v1)
        k2_dis = x2+(i*v2)
        if (k1_dis==k2_dis):
            Exist = True
            break
        else:
            pass
    if Exist:
        return "YES"
    else:
        return "NO"
